{
  "name": "serverless-microservices",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "AWS Serverless Microservices Blueprint with CDK and TypeScript",
  "scripts": {
    "build": "pnpm -r run build",
    "lint": "pnpm -r run lint",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "typecheck": "pnpm -r run typecheck",
    "clean": "pnpm -r run clean && rm -rf node_modules",
    "deploy:all": "./scripts/deploy-all.sh",
    "deploy:all:prod": "./scripts/deploy-all.sh prod",
    "deploy:fast": "./scripts/fast-deploy.sh",
    "deploy:fast:prod": "./scripts/fast-deploy.sh prod",
    "deploy:stack": "./scripts/deploy-stack.sh"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/node": "^22.16.5",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.8.0",
    "prettier": "^3.3.0",
    "rimraf": "^6.0.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.13.1"
}
